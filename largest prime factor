public: 
    long long int largestPrimeFactor(int N){
        // code here
        
        int largestPrime=-1;
       for(int i=2;i*i<=N;i++)
       
       {
           if(N%i==0)
           {
               while(N%i==0)
               {
                   largestPrime=i;
                   
                   N=N/i;
               }
           }
           
       }
       
       if(N>1)
       
           largestPrime=N;
           
           
       return largestPrime;
   }
   
};
